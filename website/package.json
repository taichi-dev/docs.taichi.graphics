{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider docusaurus start",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider yarn run validateplugin && docusaurus build",
    "swizzle": "cross-env NODE_OPTIONS=--openssl-legacy-provider docusaurus swizzle",
    "deploy": "cross-env NODE_OPTIONS=--openssl-legacy-provider docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider docusaurus serve",
    "write-translations": "WRITING_TRANSLATION=1 docusaurus write-translations",
    "write-heading-ids": "WRITING_TRANSLATION=1 docusaurus write-heading-ids",
    "crowdin": "crowdin",
    "crowdin:sync": "WRITING_TRANSLATION=1 docusaurus write-translations && while true; do if (crowdin upload && crowdin download); then break; fi; echo Wait!; sleep 300; done",
    "crowdin:download": "WRITING_TRANSLATION=1 docusaurus write-translations && crowdin download",
    "crowdin:upload": "WRITING_TRANSLATION=1 docusaurus write-translations && crowdin upload",
    "genversions": "node ./scripts/versions",
    "validateplugin": "node ./scripts/validation/index.js"
  },
  "dependencies": {
    "@crowdin/cli": "^3.6.4",
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@giscus/react": "^2.0.6",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "algoliasearch-helper": "^3.5.5",
    "autoprefixer": "^10.4.7",
    "bootstrap": "^5.1.3",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "globby": "^11.1.0",
    "jsonp": "^0.2.1",
    "postcss": "^8.4.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark": "^13.0.0",
    "semver": "^7.3.7",
    "tailwindcss": "^3.1.6",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.15",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.11",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "docusaurus-plugin-sass": "^0.2.1",
    "sass": "^1.48.0",
    "typescript": "^4.3.2"
  }
}